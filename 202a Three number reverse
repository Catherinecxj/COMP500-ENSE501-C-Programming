# include <stdio.h>

int main(void){

	int number1 = 1;
	int number2 = 1;
	int number3 = 1;
	printf("Please enter number 1:");
	scanf("%d",&number1);
	printf("Please enter number 2:");
	scanf("%d",&number2);
	printf("Please enter number 3:");
	scanf("%d",&number3);

	printf("Your numbers in entry order:\n");
	printf("%d\n",number1);
	printf("%d\n",number2);
	printf("%d\n",number3);

	printf("Your numbers reversed:\n");
	printf("%d\n", number3);
	printf("%d\n", number2);
	printf("%d\n", number1);

	return 0;

}
